{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":pinDevDependencies",
    "helpers:pinGitHubActionDigests",
    "github>ansasi/renovate-config:autoMerge.json5",
    "github>ansasi/renovate-config:labels.json5",
    "github>ansasi/renovate-config:semanticCommits.json5",
    ":automergeBranch",
    ":dependencyDashboard",
    ":disableRateLimiting",
    ":timezone(Europe/Madrid)"
  ],
  "dependencyDashboardTitle": "Renovate Dashboard 🤖🛠️",
  "suppressNotifications": [
    "prEditedNotification",
    "prIgnoreNotification"
  ]
}